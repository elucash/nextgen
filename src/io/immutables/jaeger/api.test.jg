// Rocketspace API
api {
  title: "Rocketspace API"
  host: "http://derevitour.touridev.com"
  base: "/dev"
  version: "1.0.0"
  schemes: ["https"]
  produces: ["application/json"]
  consumes: ["application/json"]
}

// This is resource
resource "/rooms/{id}"
get roomDetails {
  @query id String
  @query begin Date
  @query end Date 
  @request start Date
  @response 401 Type
  @response 201 Type
}
